		"custom": {
			"gap": {
				"baseline": "15px",
				"horizontal": "min(30px, 5vw)",
				"vertical": "min(30px, 5vw)"
			},
			"spacing": {
				"horizontal": "clamp(1.51rem, 1.412rem + 0.49vw, 2rem)",
				"large": "clamp(4rem, 10vw, 8rem)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"small": "max(1.25rem, 5vw)",
				"vertical": "clamp(0.5rem, 0.45rem + 0.25vw, 0.75rem)"
			},
			"typography": {
				"font-size": {
					"heading-five": "clamp(22px,1.3vw,18px)",
					"heading-four": "clamp(18px,1.5vw,20px)",
					"heading-one": "clamp(36px,3vw,45px)",
					"heading-six": "16px",
					"heading-three": "clamp(20px,1.9vw,22px)",
					"heading-two": "clamp(28px,2.6vw,32px)"
				},
				"font-weight": {
					"bold": 700,
					"extra-bold": 900,
					"light": 300,
					"medium": 500,
